x=input("Bir sanayi adı giriniz:")
if x=="Gıda Sanayi":
  akm=float(input("AKM değeri giriniz:"))
  pH=float(input("pH değeri giriniz:"))
  YVG=float(input("Yağ ve Gres değerleri giriniz:"))
  renk=float(input("Renk değeri giriniz:"))
  ÇKM=float(input("Çökebilir katı madde değeri giriniz:"))
  ZSF=float(input("ZSF değeri giriniz:"))
  koi=float(input("KOİ değeri giriniz:"))

  if koi<=160 and akm<=120 and 6<=pH<=9 and YVG<=60 and renk<=280 and ÇKM<=0.5 and ZSF<=4:
   print("Deşarj Standartlarına uygun")
  else:
   print("Deşarj Standartlarına uygun değil çünkü;")
  if koi>160:
   print("Koi değeri standartlara uygun değil")
  if akm>120:
   print("AKM değeri standartlara uygun değil")
  if pH<6 and pH>=9:
   print("pH değeri standartlara uygun değil")
  if YVG>60:
   print("Yağ ve Gres değeri standartlara uygun değil")
  if renk>280:
   print("Renk değeri standartlara uygun değil")
  if ÇKM>0.5:
   print("Çökebilir katı madde değeri standartlara uygun değil")
  if ZSF>4:
   print("ZSF değeri standartlara uygun değil")
